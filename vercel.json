{
  "version": 2,
  "builds": [
    {
      "src": "RubikLog/RubikLog/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "RubikLog/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "RubikLog/RubikLog/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "RubikLog/frontend/build/$1"
    }
  ]
}
